@model MyBikeApp.Models.SharedViewModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Station</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.m_Station.Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayName(Model.TripsStart().ToString())
        </dd>

    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.m_Station.Name">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
